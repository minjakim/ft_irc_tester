
one
|join #channel

:one!user@localhost JOIN #channel
:ft_ircd 331 one #channel :No topic is set
:ft_ircd 353 one = #channel :@one 
:ft_ircd 366 one #channel :End of NAMES list
:two!user@localhost JOIN #channel
:three!user@localhost JOIN #channel
:three!user@localhost PART #channel :three
:three!user@localhost JOIN #channel

two
|join #channel|invite three #channel

:two!user@localhost JOIN #channel
:ft_ircd 331 two #channel :No topic is set
:ft_ircd 353 two = #channel :@one two 
:ft_ircd 366 two #channel :End of NAMES list
:ft_ircd 341 two three #channel
:three!user@localhost JOIN #channel
:three!user@localhost PART #channel :three
:three!user@localhost JOIN #channel

three
|join #channel|part #channel|join #channel

:two!user@localhost INVITE three #channel
:three!user@localhost JOIN #channel
:ft_ircd 331 three #channel :No topic is set
:ft_ircd 353 three = #channel :@one two three 
:ft_ircd 366 three #channel :End of NAMES list
:three!user@localhost PART #channel :three
:three!user@localhost JOIN #channel
:ft_ircd 331 three #channel :No topic is set
:ft_ircd 353 three = #channel :@one two three 
:ft_ircd 366 three #channel :End of NAMES list
